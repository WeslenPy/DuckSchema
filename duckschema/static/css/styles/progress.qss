QProgressBar {
	
	background-color: #dedede;
	color: #000;
	border-style: none;
	border-radius: 5px;
	text-align: center;
    /* margin-top:7px; */
}
QProgressBar::chunk{
	border-radius: 5px;
	background-color: qlineargradient(spread:pad, x1:0, y1:0.511364, x2:1, y2:0.523, stop:0 rgba(144, 238, 144, 1), stop:1 rgba(0, 100, 45, 0.9));
}